{
    "name": "vagrant/project",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "3.2.*",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "symfony/swiftmailer-bundle": "^2.3.10",
        "symfony/monolog-bundle": "^3.0.2",
        "symfony/polyfill-apcu": "^1.0",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "^2.0",
        "snc/redis-bundle": "2.0.0",
        "predis/predis": "^1.1",
        "doctrine/doctrine-migrations-bundle": "^1.1",
        "friendsofsymfony/rest-bundle": "^2.1",
        "stof/doctrine-extensions-bundle": "^1.2",
        "nelmio/api-doc-bundle": "^2.13",
        "lexik/jwt-authentication-bundle": "^1.6",
        "liip/functional-test-bundle": "^1.7",
        "jms/serializer-bundle": "^1.2",
        "sonata-project/doctrine-orm-admin-bundle": "^3.1",
        "sonata-project/admin-bundle": "^3.13",
        "white-october/pagerfanta-bundle": "^1.0",
        "league/glide-symfony": "^1.0",
        "knplabs/knp-snappy-bundle": "^1.4",
        "knplabs/knp-snappy": "^0.4.3",
        "symfony/assetic-bundle": "^2.8",
        "sonata-project/media-bundle": "^3.5",
        "tecnickcom/tc-lib-pdf-encrypt": "^1.5",
        "tecnickcom/tc-lib-pdf-page": "^2.4",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "h4cc/wkhtmltoimage-amd64": "0.12.x"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0",
        "laravel/homestead": "^4.0",
        "phpunit/phpunit": "5.1.*",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "nelmio/alice": "^2.1",
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml",
            "env-map": {
                "database_name": "APP_DB_DATABASE",
                "database_user": "APP_DB_USERNAME",
                "database_password": "APP_DB_PASSWORD",
                "secret": "APP_SECRET_KEY",
                "jwt_key_pass_phrase": "APP_JWT_KEY_PASS_PHRASE"
            }
        },
        "branch-alias": null
    }
}
